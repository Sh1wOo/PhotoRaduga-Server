generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int @id @default(autoincrement())
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt

  email   String @unique
  password String  

  name String? @unique
  avatar String @default()
}
